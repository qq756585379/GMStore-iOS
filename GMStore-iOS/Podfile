    pod 'SDWebImage'
    pod 'TZImagePickerController'
    pod 'IQKeyboardManager'
    pod 'AFNetworking'
    pod 'CDDPagerController'
    pod 'PureLayout'
    pod 'PPNumberButton'
    
    
    pod 'YJHomeKit', :path=> '../../YJHomeKit'
#    pod 'YJHomeKit'

end
